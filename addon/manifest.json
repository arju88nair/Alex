{
  "manifest_version": 2,
  "name": "MyAddon",
  "version": "1.0",
  "description": "Captures content of web page and sends it to backend API",
  "permissions": [
    "tabs",
    "activeTab",
    "webRequest",
    "webRequestBlocking",
    "storage",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "contentScript.js"
      ]
    }
  ]
}